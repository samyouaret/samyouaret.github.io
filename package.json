  "bugs": {
    "url": "https://github.com/samyouaret/samyouaret.github.io/issues"
  },
  "homepage": "https://github.com/samyouaret/samyouaret.github.io",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@fullhuman/postcss-purgecss": "^4.0.0",
    "autoprefixer": "^10.2.4",
    "cssnano": "^4.1.10",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^7.0.2",
    "postcss": "^8.2.10",
    "postcss-cli": "^8.3.1",
