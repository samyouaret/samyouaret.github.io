{
  "name": "ouaret-samy-blog",
  "version": "2.0.0",
  "description": "personal blog of Samy Ouaret",
  "scripts": {
    "buildcss": "postcss src/css/*.css --base --dir public/static/css --env=prod",
    "watchcss": "postcss src/css/*.css --base --dir public/static/css --watch --env=dev",
    "watcheleventy": "eleventy --watch",
    "serve": "eleventy --serve",
    "debugeleventy": "DEBUG=* eleventy",
    "buildeleventy": "eleventy",
    "build": "postcss src/css/*.css --base --dir public/static/css --env=prod && eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samyouaret/samyouaret.github.io.git"
  },
  "keywords": [
    "Ouaret Samy",
